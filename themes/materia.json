{
    "$schema": "vscode://schemas/color-theme",
    "name": "materia",
    "type": "dark",
    "include": "./tokens-dark.json",
    "colors": {
        // Terminal
        "terminal.ansiWhite": "#9E9E9E",
        "terminal.ansiBlue": "#3761A4",
        "terminal.ansiBrightBlue": "#6C9AE5",
        "terminal.ansiCyan": "#0097A7",
        "terminal.ansiBrightCyan": "#26C6DA",
        "terminal.ansiGreen": "#5a8744",
        "terminal.ansiBrightGreen": "#88C36A",
        "terminal.ansiMagenta": "#9C27B0",
        "terminal.ansiBrightMagenta": "#CE93D8",
        "terminal.ansiRed": "#B71C1C",
        "terminal.ansiBrightRed": "#FF5252",
        "terminal.ansiYellow": "#CC8820",
        "terminal.ansiBrightYellow": "#FFCC80",
        // Backgrounds
        "activityBar.background": "#2D2D2D",
        "sideBar.background": "#2D2D2D",
        "statusBar.background": "#2D2D2D",
        "editor.background": "#2D2D2D",
        "panel.background": "#2D2D2D",
        // Void/transparent
        "terminal.border": "#0000",
        "statusBarItem.remoteBackground": "#0000",
        "sideBarSectionHeader.background": "#0000",
        "tab.activeBackground": "#0000",
        "tab.border": "#0000",
        "tab.inactiveBackground": "#0000",
        "editorGroupHeader.tabsBackground": "#0000",
        // Slightly darker
        "panel.border": "#0004",
        "sideBar.border": "#0004",
        "statusBar.border": "#0004",
        "editorGroup.border": "#0004",
        "editor.lineHighlightBackground": "#0004",
        // Slightly lighter
        "focusBorder": "#FFF1",
        "editorRuler.foreground": "#FFF1",
        "editor.selectionBackground": "#FFF1",
        // Dim
        "editorLineNumber.foreground": "#FFF3",
        "list.activeSelectionBackground": "#FFF3",
        // Gray
        "titleBar.inactiveForeground": "#FFF8",
        "quickInput.foreground": "#FFF8",
        "sideBar.foreground": "#FFF8",
        "statusBar.foreground": "#FFF8",
        "statusBarItem.remoteForeground": "#FFF8",
        // Blue-30 - #0079ce
        // Blue-15 - #0092fa
        // Blue - #28A6FF
        "tab.activeBorder": "#28A6FF",
        "panelTitle.activeBorder": "#28A6FF",
    }
}
