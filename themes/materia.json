        "statusBar.border": "#242424",
        "activityBar.border": "#242424",
        "editorHoverWidget.border": "#242424",
		"editorSuggestWidget.border": "#242424",
        "editorGroupHeader.border": "#242424",
        "peekView.border": "#242424",

        // Ignored/transparent
        "terminal.border": "#0000",
        "statusBarItem.remoteBackground": "#0000",
        "sideBarSectionHeader.background": "#0000",
        "tab.activeBackground": "#0000",
        "tab.border": "#0000",
        "tab.inactiveBackground": "#0000",
